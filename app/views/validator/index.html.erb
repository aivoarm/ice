<script>
         
  $(document).ready(function(){
      
       
      $('#data').html("<h4 style='color:black'>Layout validation</h4>")
      
     var obj1 = <%= @fheader_layout.html_safe %>
     var obj2 = <%= @iheader_layout.html_safe %>
     var obj3 = <%= @idetails_layout.html_safe %>
   
   
   
  
       
    $('#data').append( to_each(obj1).join(''));
    $('#data').append( to_each(obj2).join(''));
    $('#data').append( to_each(obj3).join(''));
  
      
  });
  
 
 
 function to_each(obj){
       var items = [];
     $.each(obj, function(index, message)    {
        
        $.each(message, function(i, item) {
            if (item[0]== null || item[0] ){   }
                else{
              items.push('<li>'+item[0]+' : '+item[1] + '</li>');
            }
             
        });
   });  // close each()
 
  
  
     return items
  
 }


</script>

